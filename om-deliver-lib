

OM_PATH="./"
LIBS_FOLDER="/Users/bresson/SRC/omlibraries"

LWARCH=64-bit
LWVERSION=7.1
LWEXEC=lispworks-7-1-0-amd64-darwin

# #P\"${PWD}/$1/\")"

echo "(setf *lib-name* #P\"$LIBS_FOLDER/$1/\")" > 'lib-name.lisp.tmp'

/Applications/LispWorks\ $LWVERSION\ \($LWARCH\)/LispWorks\ \($LWARCH\).app/Contents/MacOS/$LWEXEC -build $OM_PATH"/OPENMUSIC/build/build-om/pack-one-library.lisp"

rm 'lib-name.lisp.tmp'